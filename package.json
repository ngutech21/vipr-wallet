{
  "name": "vipr",
  "version": "0.0.1",
  "description": "Fedimint eCash Wallet",
  "productName": "Vipr-Wallet",
  "author": "ngutech21 <ngutech21@pm.me>",
  "type": "module",
  "private": true,
  "scripts": {
    "lint": "eslint -c ./eslint.config.js \"./src*/**/*.{ts,js,cjs,mjs,vue}\"",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "pnpm run test:unit",
    "dev": "quasar dev -m pwa",
    "build": "quasar build -m pwa",
    "postinstall": "quasar prepare",
    "test:unit:ui": "vitest --ui",
    "test:unit": "vitest",
    "test:unit:ci": "vitest run",
    "typecheck": "exec vue-tsc --noEmit",
    "final-check": "pnpm format && pnpm lint && pnpm typecheck && pnpm test:unit:ci",
    "build-docker": "docker build -t vipr-wallet ."
  },
  "dependencies": {
    "@fedimint/core-web": "0.0.0-canary-5fb29a5c5da7822f15f56d422ef835278d47cbf3",
    "@getalby/bitcoin-connect": "^3.9.2",
    "@getalby/lightning-tools": "^6.0.0",
    "@nostr-dev-kit/ndk": "^2.14.33",
    "@quasar/extras": "^1.17.0",
    "@vueuse/core": "^13.6.0",
    "bech32": "^2.0.0",
    "idb": "^8.0.3",
    "js-confetti": "^0.12.0",
    "qrcode.vue": "^3.6.0",
    "quasar": "^2.18.2",
    "register-service-worker": "^1.7.2",
    "uuid": "^11.1.0",
    "vue": "^3.5.18",
    "vue-qrcode-reader": "^5.7.3",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@pinia/testing": "^1.0.2",
    "@quasar/app-vite": "^2.3.0",
    "@quasar/cli": "^2.5.0",
    "@quasar/quasar-app-extension-testing-unit-vitest": "^1.2.3",
    "@types/node": "^24.2.0",
    "@vitest/ui": "^3.1.1",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.32.0",
    "eslint-plugin-vue": "^10.4.0",
    "globals": "^16.3.0",
    "pinia": "^3.0.3",
    "prettier": "^3.6.2",
    "typescript": "~5.8.3",
    "vite": "^6.3.5",
    "vite-plugin-checker": "^0.10.2",
    "vite-plugin-top-level-await": "^1.6.0",
    "vite-plugin-wasm": "^3.5.0",
    "vitest": "^3.1.1",
    "vue-tsc": "^3.0.5",
    "workbox-build": "^7.3.0",
    "workbox-cacheable-response": "^7.3.0",
    "workbox-core": "^7.3.0",
    "workbox-expiration": "^7.3.0",
    "workbox-precaching": "^7.3.0",
    "workbox-routing": "^7.3.0",
    "workbox-strategies": "^7.3.0"
  },
  "engines": {
    "node": "^28 || ^26 || ^24 || ^22 || ^20 || ^18",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "@swc/core",
      "bufferutil",
      "es5-ext",
      "esbuild",
      "utf-8-validate"
    ],
    "overrides": {
      "http-proxy-middleware": "^3.0.5"
    }
  }
}
