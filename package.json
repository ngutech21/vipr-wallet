{
  "name": "vipr",
  "version": "0.0.1",
  "description": "Fedimint eCash Wallet",
  "productName": "Vipr-Wallet",
  "author": "ngutech21 <ngutech21@pm.me>",
  "type": "module",
  "private": true,
  "scripts": {
    "lint": "eslint -c ./eslint.config.js \"{src,test}/**/*.{ts,js,cjs,mjs,vue}\"",
    "lint:fix": "eslint -c ./eslint.config.js \"{src,test}/**/*.{ts,js,cjs,mjs,vue}\" --fix",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "pnpm run test:unit:ci",
    "dev": "quasar dev -m pwa",
    "dev:e2e": "BROWSER=none quasar dev -m pwa --port 9303",
    "build": "quasar build -m pwa",
    "build:docker": "docker build -t vipr-wallet .",
    "postinstall": "quasar prepare",
    "coverage": "vitest run --coverage",
    "test:setup": "bash scripts/setup_test_shell.sh",
    "test:unit:ui": "vitest --ui",
    "test:unit:ci": "vitest run",
    "test:e2e": "pnpm run test:setup playwright test",
    "test:e2e:headed": "playwright test --headed",
    "typecheck": "exec vue-tsc --noEmit",
    "run-devimint": "pnpm run test:setup playwright test",
    "final-check": "pnpm format && pnpm lint && pnpm typecheck && pnpm test:unit:ci"
  },
  "dependencies": {
    "@fedimint/core": "0.1.1",
    "@fedimint/transport-web": "0.1.0",
    "@getalby/bitcoin-connect": "^3.11.5",
    "@getalby/lightning-tools": "^6.0.0",
    "@nostr-dev-kit/ndk": "^2.18.1",
    "@quasar/extras": "^1.17.0",
    "@vueuse/core": "^14.1.0",
    "bech32": "^2.0.0",
    "consola": "^3.4.2",
    "js-confetti": "^0.13.1",
    "qrcode.vue": "^3.6.0",
    "quasar": "^2.18.6",
    "register-service-worker": "^1.7.2",
    "uuid": "^13.0.0",
    "vue": "^3.5.26",
    "vue-qrcode-reader": "^5.7.3",
    "vue-router": "~4.5.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@pinia/testing": "^1.0.3",
    "@playwright/test": "1.56.1",
    "@quasar/app-vite": ">=2.0.0 <2.5.0",
    "@quasar/cli": "^2.5.0",
    "@quasar/quasar-app-extension-testing-unit-vitest": "^1.2.4",
    "@types/node": "^25.0.3",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.23",
    "eslint": "9.39.2",
    "eslint-plugin-vue": "^10.6.2",
    "globals": "^16.5.0",
    "pinia": "^3.0.4",
    "prettier": "^3.7.4",
    "tsx": "^4.21.0",
    "typescript": "~5.9.3",
    "unplugin-vue-router": "^0.15.0",
    "vite": "~6.3.7",
    "vite-plugin-checker": "^0.12.0",
    "vite-plugin-top-level-await": "^1.6.0",
    "vite-plugin-wasm": "^3.5.0",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.2.1",
    "workbox-build": "^7.4.0",
    "workbox-cacheable-response": "^7.4.0",
    "workbox-core": "^7.4.0",
    "workbox-expiration": "^7.4.0",
    "workbox-precaching": "^7.4.0",
    "workbox-routing": "^7.4.0",
    "workbox-strategies": "^7.4.0"
  },
  "engines": {
    "node": "^28 || ^26 || ^24 || ^22 || ^20 || ^18",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "@swc/core",
      "bufferutil",
      "es5-ext",
      "esbuild",
      "utf-8-validate"
    ],
    "overrides": {
      "http-proxy-middleware": "^3.0.5",
      "happy-dom": ">=20.0.2"
    }
  }
}