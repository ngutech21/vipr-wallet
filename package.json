{
  "name": "vipr",
  "version": "0.0.1",
  "description": "Fedimint eCash Wallet",
  "productName": "Vipr-Wallet",
  "author": "ngutech21 <ngutech21@pm.me>",
  "type": "module",
  "private": true,
  "scripts": {
    "lint": "eslint -c ./eslint.config.js \"./src*/**/*.{ts,js,cjs,mjs,vue}\"",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "pnpm run test:unit",
    "dev": "quasar dev -m pwa",
    "build": "quasar build -m pwa",
    "postinstall": "quasar prepare",
    "test:unit:ui": "vitest --ui",
    "test:unit": "vitest",
    "test:unit:ci": "vitest run",
    "typecheck": "exec vue-tsc --noEmit",
    "final-check": "pnpm format && pnpm lint && pnpm typecheck && pnpm test:unit:ci"
  },
  "dependencies": {
    "@fedimint/core-web": "^0.0.10",
    "@getalby/bitcoin-connect": "^3.7.1",
    "@getalby/lightning-tools": "^5.1.2",
    "@nostr-dev-kit/ndk": "^2.12.2",
    "@quasar/extras": "^1.16.17",
    "@vueuse/core": "^13.0.0",
    "idb": "^8.0.2",
    "js-confetti": "^0.12.0",
    "qrcode.vue": "^3.6.0",
    "quasar": "^2.18.1",
    "register-service-worker": "^1.7.2",
    "uuid": "^11.1.0",
    "vue": "^3.4.18",
    "vue-qrcode-reader": "^5.7.1",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@eslint/js": "^9.23.0",
    "@pinia/testing": "^1.0.0",
    "@quasar/app-vite": "^2.2.0",
    "@quasar/cli": "^2.5.0",
    "@quasar/quasar-app-extension-testing-unit-vitest": "^1.1.0",
    "@types/node": "^22.13.13",
    "@vitest/ui": "^3.0.5",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/test-utils": "^2.4.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.23.0",
    "eslint-plugin-vue": "^10.0.0",
    "globals": "^16.0.0",
    "pinia": "^3.0.1",
    "prettier": "^3.5.3",
    "typescript": "~5.8.2",
    "vite": "^6.2.3",
    "vite-plugin-checker": "^0.8.0",
    "vite-plugin-top-level-await": "^1.5.0",
    "vite-plugin-wasm": "^3.4.1",
    "vitest": "^3.0.5",
    "vue-tsc": "^2.1.10",
    "workbox-build": "^7.0.0",
    "workbox-cacheable-response": "^7.0.0",
    "workbox-core": "^7.0.0",
    "workbox-expiration": "^7.0.0",
    "workbox-precaching": "^7.0.0",
    "workbox-routing": "^7.0.0",
    "workbox-strategies": "^7.0.0"
  },
  "engines": {
    "node": "^28 || ^26 || ^24 || ^22 || ^20 || ^18",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@swc/core",
      "bufferutil",
      "es5-ext",
      "esbuild",
      "utf-8-validate"
    ]
  }
}
